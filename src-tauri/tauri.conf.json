{
    "build": {
        "beforeBuildCommand": "ionic build",
        "beforeDevCommand": "ionic serve",
        "devPath": "http://localhost:8100",
        "distDir": "../dist"
    },
    "package": {
        "productName": "crabsay",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "protocol": {
                "all": false,
                "asset": true,
                "assetScope": ["$TEMP/**/*crabsay.png"]
            },
            "os": {
                "all": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.crabsay.dev",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": ["assets/**"],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": "default-src 'self' https://whatthecommit.com/index.txt; img-src 'self' asset: https://asset.localhost; style-src 'self' 'unsafe-inline'"
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "Crabsay",
                "width": 800
            }
        ]
    }
}
